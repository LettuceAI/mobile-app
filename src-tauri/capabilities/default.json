{
  "$schema": "../gen/schemas/android-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "app-events:default",
    "process:default",
    "dialog:default",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "fs:allow-read-file"
  ]
}